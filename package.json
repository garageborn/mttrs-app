  "scripts": {
    "start-web": "DEBUG=true ./node_modules/nodemon/bin/nodemon.js --exec ./node_modules/babel-cli/bin/babel-node.js ./app/web/server/server.js -w ./app",
    "build-web-webpack": "./node_modules/webpack/bin/webpack.js -p --config ./app/web/server/webpack.production.js --progress --colors",
    "build-web-babel": "./node_modules/babel-cli/bin/babel.js ./app --out-dir ./build --source-maps --copy-files",
    "build-web": "npm run build-web-webpack && npm run build-web-babel",
    "start-native": "node node_modules/react-native/local-cli/cli.js start"
  },
